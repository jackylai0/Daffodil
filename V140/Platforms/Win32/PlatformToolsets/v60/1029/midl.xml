  </StringProperty>
  <StringProperty Subtype="file" Name="ProxyFileName" DisplayName="Soubor proxy" Description="Určuje název souboru proxy. (/proxy název_souboru)" Category="Output" Switch="proxy" Separator=" " F1Keyword="VC.Project.VCMidlTool.ProxyFileName">
  </StringProperty>
