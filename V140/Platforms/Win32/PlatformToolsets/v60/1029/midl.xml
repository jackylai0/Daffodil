    <EnumValue Name="Stub" Switch="server stub" DisplayName="Zástupná procedura" Description="Zástupná procedura">
    </EnumValue>
    <EnumValue Name="None" Switch="server none" DisplayName="Žádný" Description="Žádný">
    </EnumValue>
  </EnumProperty>
