    <EnumValue Name="Stub" Switch="client stub" DisplayName="Zástupná procedura" Description="Zástupná procedura">
    </EnumValue>
    <EnumValue Name="None" Switch="client none" DisplayName="Žádný" Description="Žádný">
    </EnumValue>
  </EnumProperty>
