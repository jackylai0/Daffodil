  </IntProperty>
  <StringProperty Name="OutputDirectory" DisplayName="Výstupní adresář" Description="Výstupní adresář (/out [adresář])" Category="Output" Switch="out" F1Keyword="VC.Project.VCMidlTool.OutputDirectory">
  </StringProperty>
